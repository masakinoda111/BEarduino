void loop()
{
  Blynk.run();
  
   x = y = z = 0;
   for (i=0; i<50; i++) {
    x = x + analogRead(1);
    y = y + analogRead(2);
    z = z + analogRead(3);
   }
   x = x / 50;
   y = y / 50;
   z = z / 50;

  xbuf[counter] = x;
  ybuf[counter] = y;
  zbuf[counter] = z;
  counter++;

  if ( cnt < 40) cnt++;

  if ( counter == 40) {
    counter = 0;
    flag = 1;
  }
  if (flag == 1) {
    xave = xaverage();
    yave = yaverage();
    zave = zaverage();
  }
  if ( (abs(x - xave)>15) || (abs(y - yave)>15) || (abs(z-zave)>15)) { 
    if (xave != 0) {
      if (cnt>=40) {
      play();
      cnt = 0;
      }
    }
   }

   int rotatex = (x-201)/1.517 - 90;
   Serial.print("X:");
   Serial.print(x);
   Serial.print(" ");
   Serial.print(rotatex);
   Serial.print("度");
   int rotatey = (y-205)/1.522 - 90;
   Serial.print("   Y:");
   Serial.print(y);
   Serial.print(" ");
   Serial.print(rotatey);
   Serial.println("度");
